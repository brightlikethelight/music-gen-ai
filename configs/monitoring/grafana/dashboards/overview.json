{
  "dashboard": {
    "id": null,
    "title": "Music Gen AI - Overview Dashboard",
    "tags": ["musicgen", "overview", "production"],
    "style": "dark",
    "timezone": "UTC",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "System Health Score",
        "type": "stat",
        "targets": [
          {
            "expr": "musicgen:system_health_score_5m",
            "legendFormat": "Health Score"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 80},
                {"color": "green", "value": 95}
              ]
            }
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Service Availability",
        "type": "stat",
        "targets": [
          {
            "expr": "musicgen:service_availability_5m * 100",
            "legendFormat": "Availability"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 99},
                {"color": "green", "value": 99.9}
              ]
            }
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "Active Alerts",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(musicgen_alerts_active)",
            "legendFormat": "Active Alerts"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 5}
              ]
            }
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0}
      },
      {
        "id": 4,
        "title": "Active Users",
        "type": "stat",
        "targets": [
          {
            "expr": "musicgen:active_users_5m",
            "legendFormat": "Active Users"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {"mode": "palette-classic"}
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 0}
      },
      {
        "id": 5,
        "title": "API Request Rate",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum(musicgen:api_request_rate_5m)",
            "legendFormat": "Total Requests/sec"
          },
          {
            "expr": "sum(musicgen:api_request_rate_5m{status_code=~\"2..\"})",
            "legendFormat": "Successful Requests/sec"
          },
          {
            "expr": "sum(musicgen:api_request_rate_5m{status_code=~\"4..\"})",
            "legendFormat": "Client Errors/sec"
          },
          {
            "expr": "sum(musicgen:api_request_rate_5m{status_code=~\"5..\"})",
            "legendFormat": "Server Errors/sec"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "custom": {
              "drawStyle": "line",
              "fillOpacity": 10
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4}
      },
      {
        "id": 6,
        "title": "API Response Times",
        "type": "timeseries",
        "targets": [
          {
            "expr": "musicgen:api_latency_p50_5m",
            "legendFormat": "P50"
          },
          {
            "expr": "musicgen:api_latency_p95_5m",
            "legendFormat": "P95"
          },
          {
            "expr": "musicgen:api_latency_p99_5m",
            "legendFormat": "P99"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "custom": {
              "drawStyle": "line"
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4}
      },
      {
        "id": 7,
        "title": "Music Generation Metrics",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum(musicgen:generation_throughput_5m)",
            "legendFormat": "Generations/sec"
          },
          {
            "expr": "musicgen:generation_success_rate_5m * 100",
            "legendFormat": "Success Rate %"
          },
          {
            "expr": "musicgen:generation_duration_avg_5m",
            "legendFormat": "Avg Duration (s)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "custom": {
              "drawStyle": "line"
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 12}
      },
      {
        "id": 8,
        "title": "System Resources",
        "type": "timeseries",
        "targets": [
          {
            "expr": "avg(musicgen:cpu_utilization_5m)",
            "legendFormat": "CPU Usage %"
          },
          {
            "expr": "avg(musicgen:memory_utilization_5m)",
            "legendFormat": "Memory Usage %"
          },
          {
            "expr": "avg(musicgen:disk_utilization_5m)",
            "legendFormat": "Disk Usage %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "custom": {
              "drawStyle": "line"
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 12}
      },
      {
        "id": 9,
        "title": "SLO Status",
        "type": "table",
        "targets": [
          {
            "expr": "musicgen_slo_current_percent",
            "format": "table",
            "instant": true
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {"Time": true, "__name__": true},
              "renameByName": {
                "slo_name": "SLO",
                "time_period": "Period",
                "Value": "Current %"
              }
            }
          }
        ],
        "fieldConfig": {
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "Current %"},
              "properties": [
                {
                  "id": "unit",
                  "value": "percent"
                },
                {
                  "id": "thresholds",
                  "value": {
                    "steps": [
                      {"color": "red", "value": 0},
                      {"color": "yellow", "value": 99},
                      {"color": "green", "value": 99.9}
                    ]
                  }
                }
              ]
            }
          ]
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 20}
      },
      {
        "id": 10,
        "title": "Error Budget Status",
        "type": "timeseries",
        "targets": [
          {
            "expr": "musicgen:error_budget_api_availability_remaining_30d * 100",
            "legendFormat": "API Availability (30d)"
          },
          {
            "expr": "musicgen:error_budget_generation_success_remaining_7d * 100",
            "legendFormat": "Generation Success (7d)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 25},
                {"color": "green", "value": 50}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 20}
      }
    ],
    "templating": {
      "list": [
        {
          "name": "instance",
          "type": "query",
          "query": "label_values(up{job=~\"musicgen.*\"}, instance)",
          "refresh": 1,
          "includeAll": true,
          "allValue": ".*",
          "multi": true
        },
        {
          "name": "model",
          "type": "query",
          "query": "label_values(musicgen_generation_requests_total, model)",
          "refresh": 1,
          "includeAll": true,
          "multi": true
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "Alerts",
          "datasource": "Prometheus",
          "expr": "ALERTS{alertname!=\"Watchdog\"}",
          "titleFormat": "{{alertname}}",
          "textFormat": "{{instance}} - {{description}}",
          "iconColor": "red"
        },
        {
          "name": "Deployments",
          "datasource": "Prometheus",
          "expr": "increase(musicgen_service_info[1m])",
          "titleFormat": "Deployment",
          "textFormat": "Service restarted",
          "iconColor": "blue"
        }
      ]
    }
  }
}